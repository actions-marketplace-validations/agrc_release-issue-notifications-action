    "@ugrc/eslint-config": "^1.2.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-plugin": "^1.0.11",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
